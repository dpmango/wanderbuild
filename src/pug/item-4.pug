---
title: Проект
---
extends shared/_layout
block content
  main.page-main
    .container--breadcrumbs
      .breadcrumbs
        a.breadcrumbs__link(href="index.html") Главная
        a.breadcrumbs__link(href="list.html") Проекты
    // PROJECT SECTION :: START
    section.list-section
      .container
        .list-section__wrap.flex-wrap
          aside.list-section__aside.list-section__aside--visible
            a.list-section__back(href="list.html").wow(data-wow-delay="1s")
              span Назад к проектам
            a.breadcrumbs__link(href="#") Проекты
            img.list-section__img(src="img/sareevo/1.jpg")
            .wow(data-wow-delay=".1s")
              h3.list-section__aside-title.list-section__aside-title--main.list-section__aside-heading Дом в стиле Ар Деко в поселке Малое Сареево
            .wow(data-wow-delay=".2s")
              h3.list-section__aside-title Площадь
              p.list-section__aside-descr 620 кв.м
            .wow(data-wow-delay=".2s")
              h3.list-section__aside-title Площадь участка
              p.list-section__aside-descr 18 сот.
            .wow(data-wow-delay=".3s")
              h3.list-section__aside-title Дата завершения
              p.list-section__aside-descr 3 квартал 2018
            .wow(data-wow-delay=".4s")
              h3.list-section__aside-title Стиль
              p.list-section__aside-descr Ар-Деко
            .wow(data-wow-delay=".4s")
              h3.list-section__aside-title Кол-во спален
              p.list-section__aside-descr 5
            .wow(data-wow-delay=".4s")
              h3.list-section__aside-title Бассейн
              p.list-section__aside-descr 10 кв.м
            .wow(data-wow-delay=".4s")
              h3.list-section__aside-title Гараж
              p.list-section__aside-descr 2 м/м
            .wow(data-wow-delay=".4s")
              h3.list-section__aside-title Архитектор
              p.list-section__aside-descr Роман Ди
          .list-section__main
            img.list-section__img.wow(data-wow-delay="0.1s" src="img/sareevo/1.jpg" srcset="img/sareevo/1@2x.jpg 2x")
            img.list-section__img.wow(data-wow-delay="0.1s" src="img/sareevo/2.jpg" srcset="img/sareevo/2@2x.jpg 2x")
            img.list-section__img.wow(data-wow-delay="0.1s" src="img/sareevo/3.jpg" srcset="img/sareevo/3@2x.jpg 2x")
            img.list-section__img.wow(data-wow-delay="0.1s" src="img/sareevo/4.jpg" srcset="img/sareevo/4@2x.jpg 2x")
